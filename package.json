    "@nestjs/testing": "^7.0.0"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test:ci --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
